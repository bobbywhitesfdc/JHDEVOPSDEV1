    <validationRules>
        <fullName>Cannot_use_master_as_default_branch</fullName>
        <active>true</active>
        <description>Cannot use master as default branch</description>
        <errorConditionFormula>copado__Main_Branch__c  = &apos;master&apos;</errorConditionFormula>
        <errorMessage>Cannot use master as default branch.  Production orgs should use PRODUCTION as their branch.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Blank_Branches_allowed</fullName>
        <active>true</active>
        <description>No Blank Branches allowed</description>
        <errorConditionFormula>StepsWithBlankBranches__c  &gt; 0 &amp;&amp;  copado__Active__c</errorConditionFormula>
        <errorMessage>No Blank Branches allowed - any steps with blank branches will disallow the flow to be active.</errorMessage>
    </validationRules>
